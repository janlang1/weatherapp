<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>WeatherPlanning</display-name>
	
	<welcome-file-list>  
		<welcome-file>HomePage.jsp</welcome-file>  
	</welcome-file-list>

	<servlet>
		<servlet-name>HomePageServlet</servlet-name>
		<servlet-class>csci310.HomePageServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HomePageServlet</servlet-name>
		<url-pattern>/HomePageServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AnalysisServlet</servlet-name>
		<servlet-class>csci310.AnalysisServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AnalysisServlet</servlet-name>
		<url-pattern>/AnalysisServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>VAAServlet</servlet-name>
		<servlet-class>csci310.VAAServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>VAAServlet</servlet-name>
		<url-pattern>/VAAServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>favorites</servlet-name>
		<servlet-class>csci310.favorites</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>favorites</servlet-name>
		<url-pattern>/favorites</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserAuthServlet</servlet-name>
		<servlet-class>csci310.UserAuthServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UserAuthServlet</servlet-name>
		<url-pattern>/UserAuthServlet</url-pattern>
	</servlet-mapping>

	

	<servlet>
		<servlet-name>likes</servlet-name>
		<servlet-class>csci310.likes</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>likes</servlet-name>
		<url-pattern>/likes</url-pattern>
	</servlet-mapping>

</web-app>